    "bvhecctrl": "^0.0.18",
    "copy-to-clipboard": "^3.3.3",
    "express": "^5.1.0",
    "md5": "^2.3.0",
    "meow": "^13.2.0",
    "navcat": "^0.1.6",
    "open": "^11.0.0",
    "publish": "^0.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
