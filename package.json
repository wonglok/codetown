		"howler": "^2.2.4",
		"lodash": "^4.17.23",
		"lowdb": "^7.0.1",
		"md5": "^2.3.0",
		"meow": "^13.2.0",
		"mkdirp": "^3.0.1",
		"navcat": "^0.1.6",
		"nodemon": "^3.1.10",
		"nprogress": "^0.2.0",
