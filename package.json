{
	"name": "codetown",
	"version": "0.0.51",
	"description": "codetown is a place to code and have fun",
	"license": "MIT",
	"repository": "wonglok/codetown",
	"author": {
		"name": "lok wong",
		"url": "https://wonglok.com"
	},
	"type": "module",
	"bin": {
		"codetown": "./cli.js"
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"docker": "docker compose up --build",
		"docker:publish": "docker compose build; docker tag codetown-pod:latest wonglok831/effectnode-pod:latest; docker push wonglok831/effectnode-pod:latest;",
		"deploy": "npm run build; npm publish --access public",
		"start": "npx vite build; NODE_ENV=production node ./cli.js --host 0.0.0.0",
		"build": "NODE_ENV=production npx vite build; npx bump; ",
		"dev": "NODE_ENV=development npx nodemon ./cli.js --watch ./src",
		"test": "node ./cli.js"
	},
	"files": [
		"dist",
		"./src/backend",
		"./cli.js"
	],
	"keywords": [
		"cli",
		"codetown"
	],
	"dependencies": {
		"@ai-sdk/openai-compatible": "^2.0.30",
